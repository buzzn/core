.col-xs-12.col-sm-12.col-md-12.col-lg-6.meter{id: "meter_#{meter.id}"}
  = link_to edit_meter_path(meter),{:remote => true, :class => 'start_modal','data-toggle' => "modal",'data-target' => "#myModal"} do
    .panel.panel-colorful
      .panel-body.text-center
        %p.text-uppercase.mar-btm.text-sm
        %i.fa.fa-tachometer.fa-5x
        %hr
        %h4= meter.name
