#content
  .forminput.box
    %h1= t'contracting_party_organizations_address'

    = simple_form_for @address, url: url_for( action: 'contracting_party_organization_address_update', controller: 'wizard_consumers'), method: 'put'  do |f|
      = render 'addresses/form_fields', f: f
      = f.submit t("continue"), class: 'btn btn-success', id: 'submit'


