#content
  .forminput.box
    %h1= t'location_habitation'

    = simple_form_for @location, url: url_for( action: 'location_habitation_update', controller: 'wizard_consumers'), method: 'put'  do |f|
      = f.input :new_habitation
      = f.input :inhabited_since
      = f.submit t("continue"), class: 'btn btn-success', id: 'submit'